  "version": "0.0.1",
  "manifest_version": 3,

  "permissions": ["storage", "activeTab", "scripting", "contentSettings"],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": {
      "16": "logo.png"
    }
  }
}
